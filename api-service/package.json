{
  "name": "@ladesa-ro/api-service",
  "version": "0.0.0-freezed.0",
  "private": true,
  "scripts": {
    "build": "wireit",
    "prebuild": "rimraf dist",
    "generate:openapi": "wireit",
    "check:static": "tsc --noEmit",
    "start": "nest start -e 'node --experimental-require-module --trace-warnings'",
    "start:dev": "nest start --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:prod": "node --experimental-require-module dist/src/main",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "typeorm:ds-migration": "tsx ./node_modules/typeorm/cli.js -d src/infrastructure/integrations/database/typeorm/data-sources/migration.data-source.ts",
    "typeorm:entity": "pnpm run typeorm -- entity:create",
    "typeorm:generate": "pnpm run typeorm -- migration:generate",
    "typeorm:create": "pnpm run typeorm -- migration:create",
    "migration:run": "pnpm run typeorm:ds-migration -- migration:run",
    "migration:revert": "pnpm run typeorm:ds-migration -- migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk --import tsx -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "plop": "npx esbuild .plop/plopfile.ts --format=esm --external:inquirer --external:ast-types --external:magicast --platform=node --bundle --outfile=plopfile.mjs 1>/dev/null && plop && npm run format:write"
  },
  "wireit": {
    "build": {
      "command": "nest build",
      "files": ["tsconfig*", "src", "version.json"],
      "output": ["dist"]
    },
    "generate:openapi": {
      "command": "node --experimental-require-module dist/src/infrastructure/fixtures/scripts/codegen-openapi-json",
      "dependencies": ["build"]
    }
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@apollo/utils.keyvaluecache": "^3.1.0",
    "@keycloak/keycloak-admin-client": "^25.0.0",
    "@ladesa-ro/especificacao": "^2.1.7-next.15",
    "@ladesa-ro/especificacao-latest": "@ladesa-ro/especificacao:@latest",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.3",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.2",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.1.2",
    "@unispec/ast-builder": "^0.26.1-next.48",
    "@unispec/ast-types": "^0.26.1-next.48",
    "@unispec/ast-utils": "^0.26.1-next.48",
    "@unispec/driver-nestjs": "^0.26.1-next.48",
    "@unispec/driver-nestjs-graphql": "^0.26.1-next.48",
    "@unispec/driver-nestjs-swagger": "^0.26.1-next.48",
    "amqplib": "^0.10.4",
    "change-case": "^5.4.4",
    "compression": "^1.7.4",
    "date-fns": "^3.6.0",
    "fs-jetpack": "^5.1.0",
    "graphql": "^16.8.2",
    "graphql-list-fields": "^2.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.2.2",
    "nestjs-paginate": "^8.6.2",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.12.0",
    "rascal": "^20.1.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "tsx": "^4.15.7",
    "typeorm": "^0.3.17",
    "uuid": "^10.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/amqplib": "^0.10.5",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.17",
    "@types/graphql-list-fields": "^2.0.6",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/rascal": "^10.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "ast-types": "^0.16.1",
    "esbuild": "^0.21.5",
    "jest": "^29.5.0",
    "magicast": "^0.3.4",
    "pathe": "^1.1.2",
    "plop": "^4.0.1",
    "recast": "^0.23.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.20.0",
    "typescript": "^5.4.5",
    "wireit": "^0.14.9"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
