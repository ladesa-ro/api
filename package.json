{
  "name": "@ladesa-ro/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint:check": "tsc --noEmit; eslint -c eslint.config.mjs src",
    "lint:fix": "eslint -c eslint.config.mjs --fix src",
    "format:check": "prettier --end-of-line lf --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --end-of-line lf --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -e 'node --experimental-require-module --trace-warnings'",
    "check:static": "tsc --noEmit; npm run format:check; npm run lint:check",
    "start:dev": "nest start --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:prod": "node --experimental-require-module dist/main",
    "typeorm": "./node_modules/.bin/ts-node --transpileOnly ./node_modules/.bin/typeorm",
    "typeorm-db-migration": "./node_modules/.bin/ts-node --transpileOnly ./node_modules/.bin/typeorm -d src/integracao-banco-de-dados/typeorm/data-sources/migration.data-source.ts",
    "typeorm:entity": "npm run typeorm -- entity:create",
    "typeorm:generate": "npm run typeorm -- migration:generate",
    "typeorm:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm-db-migration -- migration:run",
    "migration:revert": "npm run typeorm-db-migration -- migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "plop": "npx esbuild .plop/plopfile.ts --format=esm --external:inquirer --external:ast-types --external:magicast --platform=node --bundle --outfile=plopfile.mjs 1>/dev/null && plop && npm run format:write",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@keycloak/keycloak-admin-client": "^24.0.5",
    "@ladesa-ro/especificacao": "^2.1.5",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@sisgea/spec": "^1.1.0-development.53",
    "@unispec/ast-builder": "^0.26.1-next.38",
    "@unispec/ast-types": "^0.26.1-next.38",
    "@unispec/ast-utils": "^0.26.1-next.38",
    "@unispec/driver-nestjs": "^0.26.1-next.38",
    "@unispec/driver-nestjs-graphql": "^0.26.1-next.38",
    "@unispec/driver-nestjs-swagger": "^0.26.1-next.38",
    "change-case": "^5.4.4",
    "compression": "^1.7.4",
    "fs-jetpack": "^5.1.0",
    "graphql": "^16.8.1",
    "graphql-list-fields": "^2.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.2.2",
    "nestjs-paginate": "^8.6.2",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.12.0",
    "rascal": "^20.1.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.17",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.3.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.17",
    "@types/graphql-list-fields": "^2.0.6",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "ast-types": "^0.16.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.4.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "magicast": "^0.3.4",
    "pathe": "^1.1.2",
    "plop": "^4.0.1",
    "prettier": "^3.3.2",
    "recast": "^0.23.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.20.0",
    "typescript-eslint": "^7.13.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "npm@10.8.1+sha512.0e9d42e92bd2318408ed81eaff2da5f78baf23ee7d12a6eed44a6e2901d0f29d7ab715d1b918ade601f72e769a824d9a5c322383f22bbbda5dd396e79de2a077"
}
