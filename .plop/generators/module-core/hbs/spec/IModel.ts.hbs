{{#if modelDated}}
import { IEntityDate } from 'application/business/(spec)';
{{/if}}

export interface I{{ c_pascal moduleName }}Model {
  {{#if (eq modelIdType "uuid") }}
  id: string;
  {{/if}}
  {{#if (eq modelIdType "numeric") }}
  id: number;
  {{/if}}

  //

  {{#each propriedadesDeclaradas}}
  // {{this.descricao}}
  {{this.nome}}: {{this.tipoInterface}} {{ ternary this.nullable "| null" "" }};

  {{/each}}
  //
  {{#if modelDated}}

  dateCreated: IEntityDate;
  dateUpdated: IEntityDate;
  dateDeleted: null | IEntityDate;
  {{/if}}
}
