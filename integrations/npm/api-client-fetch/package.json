{
  "name": "@ladesa-ro/api-client-fetch",
  "version": "1.0.0-alpha.4",
  "main": "dist/index.js",
  "description": "LADESA - Biblioteca de integração a API - Pacote NPM.",
  "keywords": [
    "ladesa-ro",
    "api",
    "api-client",
    "fetch",
    "sisgha",
    "sisgea"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ladesa-ro/api.git"
  },
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "bugs": {
    "url": "https://github.com/ladesa-ro/api/issues"
  },
  "homepage": "https://github.com/ladesa-ro/api/tree/development/integrations/npm/api-client-fetch#readme",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "files": [
    "*.md",
    "dist/**/*.*"
  ],
  "wireit": {
    "cleanup:generated": {
      "command": "rimraf ./src/http/generated"
    },
    "test": {
      "command": "npx tsc --noEmit"
    },
    "dev": {
      "command": "tsc -p tsconfig.json --watch"
    },
    "build": {
      "command": "tsc -p tsconfig.json"
    },
    "generate:api": {
      "command": "openapi-ts",
      "dependencies": [
        "cleanup:generated"
      ]
    },
    "generate": {
      "dependencies": [
        "generate:api"
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "test": "wireit",
    "dev": "wireit",
    "build": "wireit",
    "generate": "wireit",
    "generate:api": "wireit",
    "cleanup:generated": "wireit",
    "format:write": "prettier --write --no-error-on-unmatched-pattern \"{src,apps,libs,test}/**/*.ts\"",
    "release:base": "./scripts/smart-release.sh"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.48.0",
    "@release-it/bumper": "^6.0.1",
    "prettier": "^3.2.5",
    "release-it": "^17.4.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.2",
    "wireit": "^0.14.4"
  }
}
